<VirtualHost *:80>
    ServerName {{item.value.host}}
    DocumentRoot "/vagrant/{{item.value.document_root}}"
    ErrorLog /var/log/httpd/{{item.value.host}}-error.log
    <Directory "/vagrant/{{item.value.document_root}}">
        Options Includes FollowSymLinks
        AllowOverride All
        Require all granted
    </Directory>
</VirtualHost>